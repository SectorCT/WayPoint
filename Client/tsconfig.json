{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@components/*": [
        "./components/*"
      ]
    },
    "typeRoots": [
      "types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
